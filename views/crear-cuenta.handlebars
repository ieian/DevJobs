<main class="contenedor">
    <form action="/crear-cuenta" method="POST" class="default-form">
        <div class="campo">
            <label>Nombre</label>
            <input type="text" name="nombre" id="" placeholder="Tu Nombre">
        </div>
        <div class="campo">
            <label>E-mail</label>
            <input type="email" name="email" id="" placeholder="Tu E-mail">
        </div>
        <div class="campo">
            <label>Password</label>
            <input type="Password" name="password" id="" placeholder="Tu Password">
        </div>
        <div class="campo">
            <label>Confirmar Password</label>
            <input type="Password" name="confirmar" id="" placeholder="Confirmar tu Password">
        </div>
        <div class="campo acciones">
            <a href="/iniciar-sesion">Volver a iniciar sesion</a>
        </div>
        <div class="campo">
            <input type="submit" value="Crear Cuenta" class="btn btn-azul">
        </div>
    </form>
</main>